@font-face {
  font-family: 'Glyphicons Halflings';
  src: url('<%= asset_data_uri "fonts/glyphicons-halflings-regular.eot" %>');
  src: url('<%= asset_data_uri "fonts/glyphicons-halflings-regular.eot" %>?#iefix') format('embedded-opentype'), url('<%= asset_data_uri "fonts/glyphicons-halflings-regular.woff" %>') format('woff'), url('<%= asset_data_uri "fonts/glyphicons-halflings-regular.ttf" %>') format('truetype'), url('<%= asset_data_uri "fonts/glyphicons-halflings-regular.svg" %>#glyphicons-halflingsregular') format('svg');
}
