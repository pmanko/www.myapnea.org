  -webkit-box-reflect: below 4px linear-gradient(175deg, rgba(255, 255, 255, 0) 60%, rgba(255, 255, 255, 0.25) 100%);
  box-reflect: below 4px linear-gradient(175deg, rgba(255, 255, 255, 0) 60%, rgba(255, 255, 255, 0.25) 100%);
  -moz-box-reflect: below 4px linear-gradient(175deg, rgba(255, 255, 255, 0) 60%, rgba(255, 255, 255, 0.25) 100%);
}

