<div class="page-header">
  <%= link_to answer_types_path, :class => 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_answer_type_path(@answer_type), :class => 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show answer_type</h1>
</div>

<dl class="dl-horizontal">
  <dt>Name:</dt>
  <dd><%= @answer_type.name %></dd>

  <dt>Data type:</dt>
  <dd><%= @answer_type.data_type %></dd>

</dl>
