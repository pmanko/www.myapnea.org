<div class="page-header">
  <%= link_to question_flows_path, :class => 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_question_flow_path(@question_flow), :class => 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show question_flow</h1>
</div>

<dl class="dl-horizontal">
  <dt>Answer option:</dt>
  <dd><%= @question_flow.answer_option_id %></dd>

  <dt>Condition:</dt>
  <dd><%= @question_flow.condition %></dd>

  <dt>Child:</dt>
  <dd><%= @question_flow.child_id %></dd>

</dl>
