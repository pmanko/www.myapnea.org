$('link[rel=stylesheet][href*="<%= @old %>"]').prop('disabled', true).remove();
$('head').append('<%= stylesheet_link_tag @new, media: 'all', 'data-turbolinks-track' => true %>');
console.log('<%= "old: #{@old} new: #{@new}" %>');
console.log('<%= link_to icon("flag"), switch_theme_path(@old), remote: true  %>');

$('#theme-switcher').html('<%= link_to (@old =~ /day/ ? icon("sun-o") : icon('moon-o')), switch_theme_path(@old), remote: true  %>');