<div class="page-header">
  <%= link_to answer_options_path, :class => 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_answer_option_path(@answer_option), :class => 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show answer_option</h1>
</div>

<dl class="dl-horizontal">
  <dt>Numeric value:</dt>
  <dd><%= @answer_option.numeric_value %></dd>

  <dt>Text value:</dt>
  <dd><%= @answer_option.text_value %></dd>

  <dt>Time value:</dt>
  <dd><%= @answer_option.time_value %></dd>

</dl>
