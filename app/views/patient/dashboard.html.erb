<div class="page-header">
  <h1>Patient#dashboard</h1>
</div>
<div class="jumbotron">
  <div class="container">
    <h1><%= current_user.full_name %></h1>
  </div>
</div>

<div class="container">
  <div class="row">
    <div class="col-md-8">
      <h2>Questionnaires</h2>
      <ul id="questionnaires">
        <% Template.each do |t| %>
          <li>t.name</li>
          <div class="progress">
            <div class="progress-bar" role="progressbar" aria-valuenow="60" aria-valuemin="0" aria-valuemax="100" style="width: 60%;">
              60%
            </div>
          </div>
        <% end %>
      </ul>
    </div>

    <div class="col-md-4">

    </div>
  </div>
</div>
