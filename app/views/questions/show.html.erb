<div class="page-header">
  <%= link_to questions_path, :class => 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_question_path(@question), :class => 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show question</h1>
</div>

<dl class="dl-horizontal">
  <dt>Question text:</dt>
  <dd><%= @question.question_text %></dd>

  <dt>Question type:</dt>
  <dd><%= @question.question_type_id %></dd>

  <dt>Answer type:</dt>
  <dd><%= @question.answer_type_id %></dd>

</dl>
